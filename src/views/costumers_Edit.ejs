    <div class="card">
        <div class="card-body">
            <form action="/editar/<%= data.id %>" method="post">
                <div class="form-group">
                    <input type="text" name="nombre" class="form-control" value="<%= data.nombre %>" >
                </div>

                <div class="form-group">
                     <input type="text" name="direccion" class="form-control" value="<%= data.direccion %>" >
