             <div class="form-group">
                 <input type="text" name="Telefono" class="form-control" value="<%= data.Telefono %>" >
             </div>

                <button type="submit" class="btn btn-primary">
                Enviar Personal
                </button>
            </form>
        </div>
    </div>
</div>
